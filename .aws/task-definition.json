{
  "containerDefinitions": [
    {
      "name": "job-scraper-frontend",
      "image": "141683853144.dkr.ecr.us-west-2.amazonaws.com/job-scraper",
      "cpu": 0,
      "portMappings": [
        {
          "name": "job-scraper-frontend-80-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/job-scraper-frontend",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/actuator/health || exit 1"
        ]
      },
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:us-west-2:141683853144:parameter/postgres_prod_connection_string"
        },
        {
          "name": "DATABASE_USERNAME",
          "valueFrom": "arn:aws:ssm:us-west-2:141683853144:parameter/postgres_prod_username"
        },
        {
          "name": "DATABASE_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-west-2:141683853144:parameter/postgres_prod_password"
        }
      ]
    }
  ],
  "family": "job-scraper-frontend",
  "executionRoleArn": "arn:aws:iam::141683853144:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}